#include <iostream>
#include <string>
#include <regex>
#include <set>
#include <cstdlib>
 
// ЁёЁё, папап, ввв, ёё, п, п, 
// п,ЁёЁё, папаб ввб, ё  
 
void main() 
{
    system("chcp 1251");
    std::string s1, s2;
    std::getline(std::getline(std::cin, s1), s2);
    std::regex reg("[а-яА-ЯёЁ[:alpha:]]+(?=$|[^а-яА-ЯёЁ[:alpha:]])");
    std::set<std::string> sst(std::sregex_token_iterator(s1.begin(), s1.end(), reg), std::sregex_token_iterator());
    for (std::sregex_token_iterator ib(s2.begin(), s2.end(), reg), ie; ib != ie; ++ib)
    {
        if (sst.find(ib->str()) != sst.end())
        {
            std::cout << ib->str() << std::endl;
        }
    }
}
